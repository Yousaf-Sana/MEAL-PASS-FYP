<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FAF8F8"
    android:id="@+id/l1"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Dashboard"
                    android:textColor="#000"
                    android:textSize="22dp"
                    android:padding="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="30dp"
                    android:textStyle="italic|bold"/>

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/mealpass2"
                    android:layout_marginRight="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="38dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="100dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="165dp"
                        android:orientation="horizontal">


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            app:cardCornerRadius="12sp"
                            android:layout_weight="0.8"
                            android:layout_gravity="center"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"
                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:id="@+id/imgWholesale"
                                    android:src="@drawable/add_image"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:id="@+id/tvWholesale"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Add Food Item"
                                    android:textColor="#000"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/imgWholesale"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>

                                <Button
                                    android:id="@+id/btnAddItem"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@android:color/transparent" />
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.02"/>


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            android:layout_weight="0.8"
                            app:cardCornerRadius="12sp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"

                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="49dp"
                                    android:layout_height="49dp"
                                    android:id="@+id/imgMarket"
                                    android:src="@drawable/check_order"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Check Orders"
                                    android:textColor="#000"
                                    android:id="@+id/tvMarket"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/imgMarket"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>
                                <Button
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/btnCheck"
                                    android:background="@android:color/transparent"/>
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="165dp"
                        android:orientation="horizontal">


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            app:cardCornerRadius="12sp"
                            android:layout_weight="0.8"
                            android:layout_gravity="center"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"

                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:id="@+id/imgRenter"
                                    android:src="@drawable/delivery_icon"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Delivery Info"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/imgRenter"
                                    android:id="@+id/tvRenter"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>
                                <Button
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/btnDelivery"
                                    android:background="@android:color/transparent"/>
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.02"/>


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            android:layout_weight="0.8"
                            android:layout_gravity="center"
                            app:cardCornerRadius="12sp"
                            android:layout_marginLeft="5dp"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"

                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.mikhaellopez.circularimageview.CircularImageView
                                    android:layout_width="52dp"
                                    app:civ_border="true"
                                    app:civ_border_color="#fff"
                                    app:civ_border_width="0.7dp"
                                    android:layout_height="52dp"
                                    android:id="@+id/imgRatelist"
                                    android:src="@drawable/pay_icon"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Set Payment"
                                    android:textColor="#000"
                                    android:id="@+id/tvRatelist"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/imgRatelist"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>

                                <Button
                                    android:id="@+id/btnPayment"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@android:color/transparent" />
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="165dp"
                        android:orientation="horizontal"
                        >


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            android:layout_weight="0.8"
                            android:layout_gravity="center"
                            app:cardCornerRadius="12sp"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"
                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:id="@+id/imgOrder"
                                    android:src="@drawable/profile_icon"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Profile"
                                    android:textColor="#000"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/imgOrder"
                                    android:id="@+id/tvStatus"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>

                                <Button
                                    android:id="@+id/btnProfile"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@android:color/transparent" />
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.02"/>

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="155dp"
                            android:layout_weight="0.8"
                            android:layout_gravity="center"
                            app:cardCornerRadius="12sp"
                            android:layout_marginLeft="5dp"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:elevation="5dp"
                            android:foreground="?android:attr/selectableItemBackground"
                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:id="@+id/img6"
                                    app:srcCompat="@drawable/inbox"
                                    android:layout_centerInParent="true"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Inbox"
                                    android:textColor="#000"
                                    android:textSize="16sp"
                                    android:layout_below="@+id/img6"
                                    android:id="@+id/tvResult"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginBottom="10dp"
                                    android:layout_marginTop="10dp"/>
                                <Button
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/btnInbox"
                                    android:background="@android:color/transparent"/>
                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</RelativeLayout>